---
import Layout from '../components/layouts/Layout.astro';
import Navbar from '../components/Navbar.astro'
import '../styles/tailwind.css'
import PDFFrame from '../components/PDFFrame.astro'
const pdfUrl = '/resume.pdf'
---

<Layout title="Resume | Portfolio">
    <Navbar />
    <div class="flex flex-col w-full max-w-screen-lg items-center justify-center my-5 mx-auto">
        <div class ="flex-1 flex items-center justify-center text-4xl text-white font-bold text-center mb-3">
            Resume
        </div>
        <PDFFrame pdfUrl={pdfUrl} />
    </div>

</Layout>